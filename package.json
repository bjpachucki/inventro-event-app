    "react": "^16.12.0",
    "react-chartjs-2": "2.9.0",
    "react-dom": "^16.12.0",
    "react-image-lightbox": "5.1.1",
    "react-numeric-input": "2.2.3",
    "react-popper": "^2.2.3",
    "react-router-dom": "^5.2.0",
    "react-scroll": "1.7.16",
    "react-toastify": "6.0.5",
    "react-transition-group": "4.4.1",
    "sass-loader": "^8.0.2"
